      "requestOperations": [
        {
          "preconditions": [],
          "operationList": [
            {
              "method": "regex_replace",
              "field": "/QPD-8-5",
              "regex": "^(\\d{5})(\\d{4})$",
              "replacement": "$1-$2"
            }
          ]
        }
      ],
      "responseOperations": [
        {
          "preconditions": [
          ],
          "operationList": [
            {
              "method": "set",
              "destinationField": "/MSH-6-1",
              "setValue": "MSH 6 NEW VALUE"
            }
          ]
        }

      ]
